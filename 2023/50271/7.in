
float max(float fx, float fy) {
    if (fx > fy)
        return fx;
    else return fy;
}

float min(float fa, float fb) {
    if(fa>fb) return fb;
    else return fa;
}

float equal(float _f1, float _f2) {
    if(_f1 == _f2)  return _f1;
    else
        return 0.0;
}

